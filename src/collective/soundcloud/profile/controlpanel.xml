<?xml version="1.0"?>
<object name="portal_controlpanel" meta_type="Plone Control Panel Tool"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="plone">

    <configlet
        title="Soundcloud Settings"
        action_id="soundcloud.settings"
        appId="SoundcloudSettings"
        category="plone-content"
        condition_expr=""
        icon_expr=""
        url_expr="string:${portal_url}/@@soundcloud-settings"
        visible="True"
        i18n:attributes="title">
            <permission>Manage Site</permission>
    </configlet>

</object>
